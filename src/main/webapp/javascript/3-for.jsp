<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
<script>
for(var i=2;i<=100;i+=2){
	document.write(i+'<br>');
}
for(var i=1;i<=100;i += 2){
	document.write(i+'<br>');
}
for(var i=3;i<=100;i += 3){
	document.write(i+'<br>');
}
var i=2;
while(var i != 100){
	var i+=2;
	document.write(i+'<br>');
}
var i=1;
while(var i != 100){
	var i+=2;
	document.write(i+'<br>');
}
var i=3;
while(var i != 100){
	var i+=3;
	document.write(i+'<br>');
}

</script>
</body>
</html>